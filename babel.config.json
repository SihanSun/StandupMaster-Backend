{
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "node": "14"
        },
        "useBuiltIns": "usage",
        "corejs": "3.6.5"
      }
    ]
  ],
  "plugins": [
    [
      "module-resolver", 
      {
        "root": ["."],
        "alias": {
          "src": "./src"
        }
      }
    ]
  ]
}